<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aituangou.po.GGoods" table="ggoods" catalog="aituangou">
        <id name="id" type="int">
            <column name="gid"/>
        </id>
        <property name="gprice" type="java.lang.Double">
            <column name="ggprice"/>
        </property>
        <property name="gnum" type="int">
            <column name="ggnum"/>
        </property>
    </class>
</hibernate-mapping>