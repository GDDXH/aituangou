<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aituangou.po.OrderList" table="orderlist" catalog="aituangou">
        <id name="oid" type="int">
            <column name="oid"/>
            <generator class="increment"/>
        </id>
        <property name="gid" type="int">
            <column name="gid"/>
        </property>
        <property name="price" type="double">
            <column name="oprice"/>
        </property>
        <property name="num" type="int">
            <column name="onum"/>
        </property>
        <property name="sumPrice" type="double">
            <column name="osumprice"/>
        </property>
        <property name="shipstate" type="java.lang.String">
            <column name="shipstate"/>
        </property>
        <property name="recstate" type="java.lang.String">
            <column name="recstate"/>
        </property>
        <property name="shopid" type="int">
            <column name="shopid"/>
        </property>
        <property name="bid" type="java.lang.String">
            <column name="bid" length="8"/>
        </property>
    </class>
</hibernate-mapping>